            "runToMain": true,
            "configFiles": [
                "${workspaceRoot}/mcu_support/stm32/stlink-dap.cfg",
                "${workspaceRoot}/mcu_support/stm32/h5xx/stm32h5x.cfg"
            ],
            "debuggerArgs": [
                "-d",
                "${workspaceRoot}",
            ],
            "preRestartCommands": [
                "add-symbol-file build/app/cli_app/cli_app",
                "load",
                "enable breakpoint",
                "monitor reset"
            ],
            "showDevDebugOutput": true,
        }
    ]
}